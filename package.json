    "babel-plugin-pipe-composition": "0.1.1",
    "babel-plugin-transform-custom-element-classes": "0.1.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-react": "^7.12.4",
    "is-callable": "^1.1.4",
    "jest": "^24.7.1",
    "jest-enzyme": "^7.0.2",
    "prop-types": "^15.6.1",
    "ramda": "^0.26.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "recompose": "^0.30.0",
