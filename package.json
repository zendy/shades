    "webpack-cli": "3.3.1",
    "webpack-node-externals": "1.7.2"
  },
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "recompose": "^0.26.0"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "crocks": "^0.11.1",
    "fast-memoize": "^2.5.1",
    "hash-it": "^4.0.4",
    "html-element-attributes": "^2.0.0",
    "html-tag-names": "^1.1.2",
    "immutable": "^4.0.0-rc.12",
    "react-html-attributes": "^1.4.6",
    "svg-tag-names": "^1.1.1",
    "tiza": "^2.2.1"
